[package]
name = "mmx-cli"
version = "0.1.0"
edition = "2021"
license = "0BSD"

[dependencies]
anyhow = "1"
clap = { version = "4", features = ["derive"] }
serde = { version = "1", features = ["derive"] }
serde_json = "1"
mmx-core = { path = "../mmx-core" }

[[bin]]
name = "mmx"
path = "src/main.rs"
